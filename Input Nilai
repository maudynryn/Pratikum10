   int Angka[n];
   for (int i=0; i<n; i++) {
        cout<<"Nilai ke-["<<i<<"] = ";
        cin>>Angka[i];

   }
        cout<<"Nilai Array = "<<endl;
        for (int i=0; i<n; i++)
        cout<<"["<<Angka[i]<<"] ";

